        </mj-column>
        <mj-column vertical-align="middle">
          <mj-text align="right" padding="0" mj-class="T9-400" color="#ababab" text-transform="uppercase" letter-spacing="2px"><a style="text-decoration:none;color:#ababab;" href="#">version en ligne&nbsp;&#x23f5;</a></mj-text>
        </mj-column>
      </mj-group>
    </mj-section>
    <mj-section mj-class="bgB" padding="5px">
      <mj-column width="100%" padding="20px 40px">
        <mj-text align="center" mj-class="T32-300 cTW" letter-spacing="2px">Découvrez notre sélection Orbi WiFi 6</mj-text>
      </mj-column>
    </mj-section>
    <mj-section mj-class="bgW" padding="0">
      <mj-column width="100%">
        <mj-image padding="0" width="700px" src="./images/ambianceOrbi.jpg" alt=""></mj-image>
      </mj-column>
    </mj-section>
    <mj-section mj-class="bgW" padding="15px 25px">
