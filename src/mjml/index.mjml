        <mj-image src="./images/logoNetgearBlack.png" alt=""></mj-image>
      </mj-column>
      <mj-column width="50%">
        <mj-social>
          <mj-social-element name="facebook" href="https://www.facebook.com/netgearfrance"></mj-social-element>
